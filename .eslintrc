{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "impliedStrict": true,
            "experimentalObjectRestSpread": false
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "node": true
    },
    "globals": {},
    "plugins": [],
    "rules": {
        "brace-style": [
            "error",
            "1tbs"
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-var": "error",
        "prefer-const": [
            "warn",
            {
                "destructuring": "any",
                "ignoreReadBeforeAssign": false
            }
        ],
        "quotes": [
            "error",
            "double",
            {
                "avoidEscape": false,
                "allowTemplateLiterals": true
            }
        ],
        "max-params": [
            "error",
            {
                "max": 4
            }
        ],
        "semi": "error",
        "arrow-body-style": [
            "error",
            "always"
        ],
        "arrow-parens": [
            "error",
            "always"
        ],
        "operator-linebreak": [
            "error",
            "before"
        ],
        "one-var": [
            "error",
            "never"
        ],
        "no-plusplus": "error",
        "no-multiple-empty-lines": "error",
        "newline-per-chained-call": "error",
        "newline-before-return": "off",
        "key-spacing": [
            "error",
            {
                "beforeColon": false,
                "afterColon": true,
                "mode": "strict"
            }
        ],
        "comma-style": [
            "error",
            "last"
        ],
        "comma-spacing": [
            "error",
            {
                "before": false,
                "after": true
            }
        ],
        "block-spacing": [
            "error",
            "always"
        ],
        "array-bracket-spacing": [
            "error",
            "never",
            {
                "objectsInArrays": true,
                "arraysInArrays": true
            }
        ],
        "dot-location": [
            "error",
            "property"
        ],
        "camelcase": "error",
        "curly": "error",
        "import/prefer-default-export": "off",
        "no-unused-expressions": "warn",
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-debugger": "off",
        "no-unsafe-negation": "error",
        "valid-jsdoc": "error",
        "accessor-pairs": "error",
        "array-callback-return": "error",
        "complexity": "off",
        "eqeqeq": "error",
        "no-caller": "error",
        "no-div-regex": "off",
        "no-empty-function": "error",
        "no-eval": "error",
        "no-extra-bind": "error",
        "no-extra-label": "error",
        "no-floating-decimal": "error",
        "no-global-assign": "error",
        "no-implied-eval": "error",
        "no-invalid-this": "off",
        "no-iterator": "error",
        "no-lone-blocks": "error",
        "no-loop-func": "error",
        "no-multi-spaces": "error",
        "no-multi-str": "error",
        "no-new-func": "error",
        "no-new-wrappers": "error",
        "no-new": "error",
        "no-octal-escape": "error",
        "no-proto": "error",
        "no-return-assign": [
            "error",
            "always"
        ],
        "no-script-url": "error",
        "no-self-compare": "error",
        "no-sequences": "error",
        "no-unmodified-loop-condition": "error",
        "no-useless-call": "error",
        "no-useless-concat": "error",
        "no-useless-escape": "error",
        "no-void": "error",
        "no-with": "error",
        "radix": "error",
        "no-catch-shadow": "error",
        "no-delete-var": "error",
        "no-label-var": "error",
        "no-shadow-restricted-names": "error",
        "comma-dangle": [
            "error",
            "never"
        ],
        "eol-last": "error",
        "func-call-spacing": "error",
        "keyword-spacing": [
            "error",
            {
                "before": true,
                "after": true
            }
        ],
        "max-depth": "off",
        "max-len": [
            "error",
            200
        ],
        "max-nested-callbacks": "error",
        "max-statements-per-line": "error",
        "new-cap": "error",
        "new-parens": "error",
        "no-array-constructor": "error",
        "no-bitwise": "error",
        "no-lonely-if": "error",
        "no-mixed-operators": "off",
        "no-new-object": "error",
        "no-tabs": "error",
        "no-trailing-spaces": "error",
        "no-unneeded-ternary": "error",
        "no-whitespace-before-property": "error",
        "padded-blocks": [
            "error",
            "never"
        ],
        "quote-props": [
            "error",
            "as-needed",
            {
                "unnecessary": false
            }
        ],
        "semi-spacing": [
            "error",
            {
                "before": false,
                "after": true
            }
        ],
        "space-before-blocks": "error",
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "space-in-parens": "error",
        "space-infix-ops": "error",
        "space-unary-ops": [
            "error",
            {
                "words": true,
                "nonwords": false,
                "overrides": {
                    "-": false
                }
            }
        ],
        "arrow-spacing": "error",
        "generator-star-spacing": [
            "error",
            {
                "before": true,
                "after": false
            }
        ],
        "no-duplicate-imports": "error",
        "no-useless-computed-key": "error",
        "no-useless-constructor": "error",
        "no-useless-rename": "error",
        "prefer-arrow-callback": [
            "off"
        ],
        "prefer-rest-params": "error",
        "prefer-spread": "error",
        "require-yield": "error",
        "rest-spread-spacing": "error",
        "symbol-description": "error",
        "template-curly-spacing": "error",
        "yield-star-spacing": [
            "error",
            {
                "before": true,
                "after": false
            }
        ]
    }
}
