        "underscore": "^1.9.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-root-import": "^6.1.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^5.5.0"
    }
}
